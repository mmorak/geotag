<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for Geotag -->

<jnlp spec="1.0+"
    codebase="http://geotag.sourceforge.net/" 
    href="geotag.jnlp">
    <information>
        <title>Geotag</title>
        <vendor>Andreas Schneider, Geotag</vendor>
        <description>Geotag Demo</description>
        <homepage href="http://geotag.sourceforge.net"/>
        <description kind="short">Geotag matches time information from photos
            with location information from GPS units</description>
        <offline-allowed/>
        <icon href="geotag-128.png" height="128" width="128"/>
        <icon kind="splash" href="geotag-128.png"/>
   </information>
   <resources>     
       <jar href="http://downloads.sourceforge.net/geotag/geotag-VERSION.jar"/>   
       <j2se version="1.6+"
            java-vm-args="-Xmx256m"
            href="http://java.sun.com/products/autodl/j2se"/>
   </resources>
   <security>
       <all-permissions/>
   </security> 
   <application-desc main-class="org.fibs.geotag.Geotag"/>
</jnlp>
